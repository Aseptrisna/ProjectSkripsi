<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menuutama"
    android:background="#FF000000">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="344dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="148dp"
        android:background="@drawable/form3">

    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="107dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="MENU"
        android:textColor="@color/colorPrimary"
        android:textSize="80dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="277dp"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="59dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="112dp"
        android:text="Project Tugas Akhir"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="30dp"
        android:textStyle="normal" />

    <Button
        android:id="@+id/btnlock"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:background="#FF000000"
        android:text="KUNCI"
        android:textAllCaps="false"
        android:textColor="#019875"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnunlock"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/btnlock"
        android:layout_alignParentTop="true"
        android:layout_marginTop="362dp"
        android:background="#FF000000"
        android:text="BUKA KUNCI"
        android:textAllCaps="false"
        android:textColor="#019875"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnlocation"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="139dp"
        android:layout_marginLeft="139dp"
        android:layout_marginTop="201dp"
        android:background="@drawable/lokasii"
        android:textAllCaps="false"
        android:textColor="#019875"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnvolt"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginTop="290dp"
        android:background="#FF000000"
        android:text="VOLT BATERAI"
        android:textAllCaps="false"
        android:textColor="#019875"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnarus"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/btnvolt"
        android:layout_alignParentTop="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="364dp"
        android:background="#FF000000"
        android:text="ARUS BATERAI"
        android:textAllCaps="false"
        android:textColor="#019875"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnuser"
        android:layout_width="72dp"
        android:layout_height="65dp"
        android:layout_below="@+id/btnvolt"
        android:layout_marginStart="-86dp"
        android:layout_marginTop="-152dp"
        android:layout_toEndOf="@+id/btnarus"
        android:background="@drawable/user" />


    <Button
        android:id="@+id/btnfoto"
        android:layout_width="60dp"
        android:layout_height="58dp"
        android:layout_below="@+id/btnvolt"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-148dp"
        android:layout_marginEnd="69dp"
        android:layout_marginRight="69dp"
        android:background="@drawable/kamera" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginBottom="2dp"
        android:text="Copyright Â© Tugas Akhir2019"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20dp"
        android:textStyle="bold" />



</RelativeLayout>